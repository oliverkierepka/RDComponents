{
  "packages": [
    "elements/*",
    "themes/*"
  ],
  "version": "independent",
  "npmClient": "npm",
  "useWorkspaces": true,
  "workspaces": [
    "elements/*",
    "themes/*"
  ],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "yes": true,
      "ignoreChanges": [
      ]
    },
    "bootstrap": {
      "ignore": "rw-dev-toolkit",
      "npmClientArgs": ["--no-package-lock"]
    }
  }
}


